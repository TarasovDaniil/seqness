{
  "name": "seqness",
  "version": "0.0.1",
  "description": "State manager",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.mjs",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prebuild": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "clean": "rm -rf ./lib",
    "build": "yarn clean && yarn prebuild && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "test": "mocha",
    "semantic-release": "semantic-release",
    "prepack": "yarn build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TarasovDaniil/seqness.git"
  },
  "bugs": {
    "url": "https://github.com/TarasovDaniil/seqness/issues"
  },
  "homepage": "https://github.com/TarasovDaniil/seqness#readme",
  "keywords": [
    "typescript",
    "state",
    "chains"
  ],
  "author": "Daniil Tarasov",
  "license": "MIT",
  "devDependencies": {
    "@types/chai-spies": "^1.0.3",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "chai-spies": "^1.0.0",
    "mocha": "^10.0.0",
    "mocha-steps": "^1.3.0",
    "prettier": "^2.8.8",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
